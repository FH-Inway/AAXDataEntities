/// the knowledge of a Customer party number.
/// </summary>
public class AAXCustomerContactPersonsEntity extends common
{

}
]]></Declaration>
		<Methods>
			<Method>
				<Name>initializeEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// The initializeEntityDataSource method will lookup the party and location numbers if needed and set on the current buffer
    /// </summary>
    /// <param name = "_entityCtx">The context of the current operation</param>
    /// <param name = "_dataSourceCtx">The context of the current data entity</param>
    /// <remarks>
    /// </remarks>
    public void initializeEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
       
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(AAXCustomerContactPersonsEntity, smmContactPersonV2Entity))
        {
            this.skipDataSourceValidateField(fieldNum(AAXCustomerContactPersonsEntity, ContactPersonId), true);

            this.setPartyNumber();

            if (_entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Update)
            {
                _dataSourceCtx.setBuffer(this.resolveContactPersonEntity());
            }
        }

        super(_entityCtx, _dataSourceCtx);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setPartyNumber</Name>
				<Source><![CDATA[
    /// <summary>
    /// The setPartyNumberAndLocationId method sets the party number and location if needed
    /// </summary>
    /// <remarks>
    /// </remarks>
    public void setPartyNumber()
    {
        CustTable custTable = CustTable::find(this.CustomerAccountNumber);
        DirPartyNumber partyNumber = DirPartyTable::findRec(custTable.Party).PartyNumber;
        this.AssociatedPartyNumber = partyNumber;
        
        if (this.PrimaryAddressLocationId == '')
        {
            DirPartyLocation dirPartyLocation = DirPartyLocation::findOrCreate(custTable.party, 0);
            this.PrimaryAddressLocationId = LogisticsLocation::find(dirPartyLocation.Location).LocationId;
        }
        
    }

]]></Source>
			</Method>
			<Method>
				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(AAXCustomerContactPersonsEntity, smmContactPersonV2Entity))
        {
            this.setPartyNumber();
        }

        super(_entityCtx, _dataSourceCtx);

    }

]]></Source>
			</Method>
			<Method>
				<Name>resolveContactPersonEntity</Name>
				<Source><![CDATA[
    /// <summary>
    /// The resolveDirPartyContactEntity finds the DirPartyContactEntity record which matches the key fields
    /// </summary>
    /// <returns>
    /// A DirPartyContactEntity matching the key fields if found
    /// </returns>
    /// <remarks>
    /// </remarks>
    private smmContactPersonV2Entity resolveContactPersonEntity()
    {
        smmContactPersonV2Entity contactPersonEntity;

        select firstonly contactPersonEntity
            where contactPersonEntity.ContactPersonId == this.contactPersonId;

        return contactPersonEntity;
    }

]]></Source>
			</Method>
